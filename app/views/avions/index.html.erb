<p id="notice"><%= notice %></p>

<h1>Avions</h1>

<table>
  <thead>
    <tr>
      <th>Matricula</th>
      <th>Nombspilot</th>
      <th>Apellspilot</th>
      <th>Destino</th>
      <th>Origen</th>
      <th>Hsalidao</th>
      <th>Fsalidao</th>
      <th>Hllegadadestino</th>
      <th>Fllegadadestino</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @avions.each do |avion| %>
      <tr>
        <td><%= avion.matricula %></td>
        <td><%= avion.nombspilot %></td>
        <td><%= avion.apellspilot %></td>
        <td><%= avion.destino %></td>
        <td><%= avion.origen %></td>
        <td><%= avion.hsalidao %></td>
        <td><%= avion.fsalidao %></td>
        <td><%= avion.hllegadadestino %></td>
        <td><%= avion.fllegadadestino %></td>
        <td><%= link_to 'Show', avion %></td>
        <td><%= link_to 'Edit', edit_avion_path(avion) %></td>
        <td><%= link_to 'Destroy', avion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Avion', new_avion_path %>
